# Monitor Dashboard Environment Configuration

# Application Settings
REACT_APP_NAME=Monitor Dashboard
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api
REACT_APP_AI_API_URL=http://localhost:8000
REACT_APP_DEPLOYMENT_API_URL=http://localhost:8001
REACT_APP_GITHUB_API_URL=https://api.github.com

# Authentication (for future implementation)
REACT_APP_AUTH_ENABLED=false
REACT_APP_AUTH_PROVIDER=oauth2
REACT_APP_AUTH_CLIENT_ID=your_client_id_here

# GitHub Integration
REACT_APP_GITHUB_ORG=your-organization
REACT_APP_GITHUB_REPO=monitor-configs
REACT_APP_GITHUB_TOKEN=your_github_token_here

# Monitoring & Analytics
REACT_APP_ANALYTICS_ENABLED=false
REACT_APP_ANALYTICS_ID=your_analytics_id_here
REACT_APP_ERROR_REPORTING_ENABLED=false
REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
REACT_APP_FEATURE_AI_FIX=true
REACT_APP_FEATURE_AUTO_DEPLOY=true
REACT_APP_FEATURE_GITHUB_INTEGRATION=true
REACT_APP_FEATURE_REAL_TIME_UPDATES=false

# UI Configuration
REACT_APP_THEME=default
REACT_APP_DEFAULT_REFRESH_INTERVAL=60000
REACT_APP_MAX_RETRIES=3
REACT_APP_REQUEST_TIMEOUT=30000 